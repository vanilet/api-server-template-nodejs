{
  "name": "api-server-template-nodejs",
  "version": "0.0.1",
  "description": "A template API-based backend server using nodejs with typescript",
  "main": "www.js",
  "repository": "\"https://github.com/vanilet/api-server-template-nodejs\"",
  "author": "Cheolmin Kim <cheolminkim@vanilet.pe.kr>",
  "license": "MIT",
